sequence: {
  shape: sequence_diagram

  dev_user: Developer
  j_opus: "Claude Code\nOpus 4.6"
  py_glm: "glm\nscript"
  ex_zai: "Z.AI\nAPI"

  # User gives task
  dev_user -> j_opus: task

  # Delegation
  delegate: {
    label: "loop [parallel agents]"
    j_opus -> py_glm: "delegate via\nBash tool"
    py_glm -> ex_zai: "claude -p\n(GLM-5)"
    ex_zai -> py_glm: result
    py_glm -> j_opus: stdout
  }

  # Synthesize
  j_opus -> dev_user: "synthesized\nresult"
}
