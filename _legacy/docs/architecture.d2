sequence: {
  shape: sequence_diagram

  dev_user: Developer
  j_opus: "Claude Code\nOpus 4.6"
  py_glm: "glm\nscript"
  j_agent: "Claude Code\nGLM-5 agent"
  ex_zai: "Z.AI\nAPI"

  # User gives task
  dev_user -> j_opus: task

  # Delegation
  delegate: {
    label: "loop [parallel agents]"
    j_opus -> py_glm: "delegate via\nBash tool"
    py_glm -> j_agent: "claude -p\n+ env vars"
    j_agent -> j_agent: "read files,\nedit, run tests,\nuse MCP & skills"
    j_agent -> ex_zai: "LLM calls\n(GLM-5)"
    ex_zai -> j_agent: response
    j_agent -> py_glm: stdout
    py_glm -> j_opus: result
  }

  # Synthesize
  j_opus -> dev_user: "synthesized\nresult"
}
